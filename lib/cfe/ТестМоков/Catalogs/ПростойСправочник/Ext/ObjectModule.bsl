&Вместо("ПередЗаписью")
Процедура тест_ПередЗаписью(Отказ)
	
	МассивПараметров = Новый Массив;
	МассивПараметров.Добавить(ЭтотОбъект);
	МассивПараметров.Добавить(Отказ);
	
	Результат = Неопределено;
	ЕстьПодмена = МокТестирование.ВыполнитьПодмену("ТестМоков.Идентификатор", МассивПараметров, Результат);
	
	Если ЕстьПодмена Тогда
		Если ТипЗнч(Результат) = Тип("Булево") Тогда
			Отказ = Результат;
		КонецЕсли;
	Иначе
		ПродолжитьВызов(Отказ);
	КонецЕсли;
	
КонецПроцедуры
